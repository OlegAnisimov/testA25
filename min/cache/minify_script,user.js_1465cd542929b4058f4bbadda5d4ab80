$('.loader-gif').hide();}else{if(typeof response.auth!=='undefined'){show_auth(response.auth);}else{check_purchase_form=false;form.find('[type=submit]').click();}}},error:function(response){console.log(response);$('.loader-gif').hide();}});}else{check_purchase_form=false;return true;}});$('input[name="delivery-address"]').change(function(){if($(this).val()=='new'){$('#new-address').show();$('.delivery_address .check').attr('required','required');}else{$('#new-address').hide();$('.delivery_address .check').removeAttr('required');}});});function show_loader(){$('.loader-gif').show();}
function trigger_delivery_change(){if($('.select-delivery-type:checked').attr('data-type')!=='self'){$('.delivery_address').show();$('#delivery-address-new').click();$('.delivery_address .check').attr('required','required');}else{$('.delivery_address .radio-button:checked').removeAttr('checked');$('.delivery_address').hide();$('.delivery_address .check').removeAttr('required');}
$('.cp-delivery-price').text($('.select-delivery-type:checked').attr('data-price')+'');var price=parseFloat($('.select-delivery-type:checked').attr('data-price'))+parseFloat($('.cp-total').attr('data-price'));$('.cp-total').text(price+'');}
function simple_basket_put(url,no_loader,product){if(no_loader===0){show_loader(no_loader);}
$.ajax({url:url,dataType:'json',success:function(response){$('.header-right-link ul li.cart-icon > a span small.cart-notification').text(response.summary.amount);animate_loader_close(no_loader);product.parents('.product-image').find(".success-in-car").stop().show().delay(4000).fadeOut();product.find(".success-in-car").stop().show().delay(4000).fadeOut();},error:function(response){animate_loader_close(no_loader);console.log(response);}});}
function animate_loader_close(no_loader){$('.loader-gif').hide();$('.add-cart-tooltip').stop().show().fadeOut(3000);if(no_loader===10){$('.loader-gif').hide();}}
function ajax_item_do(action,id,scope,button){var url='/udata/content/'+action+'/'+id+'/'+scope+'.json';$.ajax({url:url,dataType:'json',beforeSend:function(){button.attr('disabled','disabled');},success:function(){$('a[data-id="'+button.attr('data-id')+'"].'+scope).toggleClass('active');button.removeAttr('disabled');},error:function(response){button.removeAttr('disabled');console.log(response);}});}
function check_qty(qty){var old=parseInt(qty.attr('data-old')),val=parseInt(qty.val());if(val&&val>0){qty.attr('data-old',val);qty.val(val);if(qty.hasClass('quantity_cart')&&qty.hasClass('is_cart')){refresh_cart_item(qty.closest('tr.cart-item').attr('data-id'));}}else{qty.val(old);}}
var cart_change_ajax;function refresh_cart_item(item_id){show_loader();if(cart_change_ajax&&cart_change_ajax.readyState!==4){cart_change_ajax.abort();}
var block=$('#cart-item-'+item_id),amount=block.find('.quantity_cart').val(),url='/udata/emarket/refresh_cart_item/'+item_id+'/'+amount+'/.json';cart_change_ajax=$.ajax({url:url,dataType:'json',success:function(response){$('.header-right-link ul li.cart-icon > a span small.cart-notification').text(response.cart.summary.amount);block.find('.total-price .price').text(response.price+'');refresh_delivery();var id_cart=response.cart.id;$.ajax({url:'/udata/emarket/basket/'+id_cart+'.json',dataType:'json',success:function(response){refresh_cart_data(response);animate_loader_close();},error:function(response){console.log(response);}});},error:function(response){console.log(response);}});}
function refresh_delivery(){$.ajax({url:'/udata/emarket/refresh_delivery_price/.json',dataType:'json',success:function(response){$.each(response.delivery,function(key,value){$('.select-delivery-type[value='+key+']').attr('data-price',value).siblings('label').find('span').text(value);});},error:function(response){console.log(response);}});}
var delete_cart_item_ajax;function delete_cart_item(button){show_loader();if(delete_cart_item_ajax&&delete_cart_item_ajax.readyState!==4){delete_cart_item_ajax.abort();}
delete_cart_item_ajax=$.ajax({url:'/udata'+button.attr('href')+'.json',dataType:'json',success:function(response){$('.header-right-link ul li.cart-icon > a span small.cart-notification').text(response.summary.amount);if(response.summary.amount>0){button.closest('tr.cart-item').remove();refresh_delivery();trigger_delivery_change();var id_cart=response.id;$.ajax({url:'/udata/emarket/basket/'+id_cart+'.json',dataType:'json',success:function(response){refresh_cart_data(response);},error:function(response){console.log(response);}});}else{var get_back=$('.get-back-link').parent().html();$('.cart-contents').empty().html('<div class="window-message"><h4>Корзина пуста</h4><div class="mb-30"><div class="row"><div class="mt-30 text-center">'+get_back+'</div></div></div></div>');}
animate_loader_close();},error:function(response){console.log(response);}});}
function refresh_cart_data(cart){var total_price=cart.summary.price.actual,prods_price=total_price,discount=0;if(typeof cart.summary.price.original!=='undefined'){prods_price=cart.summary.price.original;}
if(typeof cart.discount_value!=='undefined'){discount=cart.discount_value;}
$('.cp-products-price').text(prods_price+'');$('.cp-discount').text(discount+'');$('.cp-total').text(total_price+'');$('.cp-total').attr('data-price',total_price);trigger_delivery_change();}
function send_form(block){var url='/udata'+block.attr('action'),data=block.serialize(),message=block.find('.form-message');console.log(data);$.ajax({url:url,data:data,dataType:'html',success:function(){if(!block.hasClass('replace-form')){message.addClass('ok').text(message.attr('data-ok'));}else{var container=block.parent(),height=block.height(),ok=message.attr('data-ok');container.empty().css('height',height).append('<div class="form-message ok text-center">'+ok+'</div>');}},error:function(response){console.log(response);message.addClass('fail').text(message.attr('data-fail'));}});}
function simple_ajax_form(block){var url='/udata'+block.attr('action')+'.json',data=block.serialize(),message=block.find('.form-message');$.ajax({url:url,data:data,dataType:'html',success:function(){var mesg=message.attr('data-ok');$('.sbs-replace').html('<div class="col-md-12"><div class="form-message ok">'+mesg+'</div></div>');},error:function(response){console.log(response);block.find('input').val(message.attr('data-fail')).parent().addClass('alert');}});}
var ajax_catalog;function get_smart_catalog(page,clear,scroll,lang_id){var form_data=$('#smart-filters').serialize(),loader=$('.ajax-loader'),content=$('.ajax-smart-catalog'),content_html=content.html(),is_abort=false,url='/udata/catalog/getSmartCatalog//'+loader.attr('data-page')+'/'+$('#sort-limit').val()+'//100/'+$('#sort-field').val()+'/'+$('#sort-order').val()+'?transform=transform/ajax_catalog.xsl&p='+page+'&lang_id='+lang_id;if(ajax_catalog&&ajax_catalog.readyState!==4){ajax_catalog.abort();}
if(!clear){url+='&'+form_data;window.history.pushState('','','?p='+page+'&'+form_data);}
ajax_catalog=$.ajax({url:url,dataType:'html',beforeSend:function(){loader.show();content.empty();if(scroll){$('body,html').animate({scrollTop:content.offset().top-180},1500);}},success:function(response){loader.hide();content.append(response);},error:function(response){console.log(response);loader.hide();if(!is_abort){content.append(content_html);}}});}
function clear_filters(){$("#smart-filters").find(".checkbox").removeAttr("checked");$("#minCost").val($("#minCost").attr('min'));$("#maxCost").val($("#maxCost").attr('max'));$("#slider-range").slider("values",0,$("#minCost").attr('min'));$("#slider-range").slider("values",1,$("#maxCost").attr('max'));}
function co_reviews_pagi(page){var loader=$('.ajax-loader'),content=$('.ajax-comments'),content_html=content.html(),parent=content.parent(),height=parent.height(),url='/udata/comments/insert/'+loader.attr('data-page')+'/?transform=transform/ajax_reviews.xsl&p='+page;$.ajax({url:url,dataType:'html',beforeSend:function(){loader.show();parent.css('height',height);content.empty();},success:function(response){loader.hide();parent.css('height','auto');content.append(response);},error:function(response){console.log(response);loader.hide();content.append(content_html);}});}
