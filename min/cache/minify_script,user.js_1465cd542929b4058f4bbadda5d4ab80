function scrolltop_arrow(){var scrollup=$('#scrollup');var headertag=$('header');var mainfix=$('.main');$(window).scroll(function(){if($(this).scrollTop()>250){scrollup.fadeIn(300);}else{scrollup.fadeOut(300);}
if($(this).scrollTop()>70){headertag.addClass("header-fixed");}else{headertag.removeClass("header-fixed");}
if($(this).scrollTop()>70){mainfix.addClass("main-fixed");}else{mainfix.removeClass("main-fixed");}});scrollup.on("click",function(){$("html, body").animate({scrollTop:0},1000);return false;});}
