return false;});$('.footer-static-block span.opener').on("click",function(){if($(this).hasClass("plus")){$(this).parent().find('.footer-block-contant').slideDown();$(this).removeClass('plus');$(this).addClass('minus');}else{$(this).parent().find('.footer-block-contant').slideUp();$(this).removeClass('minus');$(this).addClass('plus');}
return false;});}
