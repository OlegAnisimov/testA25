myJQ(window).load(function(){myJQ('.popup-form-call').magnificPopup({type:'inline'});});myJQ(window).load(function(){setTimeout(function(){if(myJQ('.check-wishlist').length>0){var url='/udata/content/get_session_page/wishlist/100/.json';myJQ.ajax({url:url,dataType:'json',success:function(response){if(parseInt(response.total)>0){myJQ.each(response.items.item,function(k,v){myJQ('.check-wishlist[data-id="'+v.id+'"]').addClass('active');});}},error:function(response){console.log(response);}});}
if(myJQ('.check-compare').length>0){var url='/udata/content/get_session_page/compare/100/.json';myJQ.ajax({url:url,dataType:'json',success:function(response){if(parseInt(response.total)>0){myJQ.each(response.items.item,function(k,v){myJQ('.check-compare[data-id="'+v.id+'"]').addClass('active');});}},error:function(response){console.log(response);}});}
if(myJQ('.widget-block').length>0){var s=document.createElement("script");s.type="text/javascript";s.src="https://vk.com/js/api/openapi.js?153";myJQ('.widget-block').append('<div id="vk_groups"></div>').append(s);setTimeout(function(){VK.Widgets.Group("vk_groups",{mode:1,no_cover:1},myJQ('.widget-block').attr('data-id'));myJQ('.podelish').html(myJQ('.podelish').attr('data-podelish'));},5000);}},3000);});var modal_loader=myJQ.magnificPopup.instance;function show_auth(email){myJQ('.loader-gif').hide();myJQ('#sug-login').val(email);myJQ.magnificPopup.open({items:{src:'#purchase-auth-suggest',type:'inline'}});}
myJQ(document).ready(function(){if(myJQ("#purchase-form-auth input[value=604]").attr('checked')=='checked'){myJQ("#purchase-form-auth").attr('target','_blank');}
myJQ("#purchase-form-auth input[name=payment-id]").on('click',function(){if(myJQ(this).val()==604){myJQ(this).parents("#purchase-form-auth").attr('target','_blank');}else{myJQ(this).parents("#purchase-form-auth").attr('target','_self');}});myJQ(document).on('click','.no-action-a',function(e){e.preventDefault();myJQ('#purchase-auth-suggest').removeClass('purchase-auth');modal_loader.close();myJQ('#purchase-form-auth').submit().find('input[type=submit]').click().trigger('click');});myJQ(document).on('submit','#purchase-auth-suggest',function(e){e.preventDefault();var url='/udata/users/try_auth_customer/.json',$this=myJQ(this);$this.find('.form-field-wrap').removeClass('alert');myJQ.ajax({method:'POST',url:url,dataType:'json',beforeSend:function(){myJQ('.loader-gif').show();},data:{login:myJQ('#sug-login').val(),password:myJQ('#sug-password').val()},success:function(response){if(response.result=='true'){modal_loader.close();myJQ('.loader-gif').hide();myJQ('#purchase-auth-suggest').removeClass('purchase-auth');myJQ('#purchase-form-auth').submit().find('input[type=submit]').click().trigger('click');}else{$this.find('.form-field-wrap').addClass('alert');myJQ('.loader-gif').hide();}},error:function(response){myJQ('.loader-gif').hide();console.log(response);}});});myJQ(document).on('submit','#delivey-address-form',function(e){e.preventDefault();var $this=myJQ(this),url='/udata/data/delivery_save/.json?'+$this.serialize();$this.find('.form-field-wrap').removeClass('alert');myJQ.ajax({url:url,dataType:'json',beforeSend:function(){myJQ('.loader-gif').show();},success:function(response){myJQ('.loader-gif').hide();if(typeof response.error!=='undefined'){myJQ.each(response.error,function(key,field_class){$this.find('.form-field-wrap.'+field_class).addClass('alert');});}else{if(myJQ('#address-id-'+response.id).length==0){myJQ('.adreses-list').append('<div class="main-form"><div class="row"><div class="col-xs-12 mb-20"><div class="heading-part delivery-list"><h2 class="heading"><p id="address-id-'+response.id+'"></p><i class="btn btn-color delivery-edit" data-id="'+response.id+'">Редактировать</i><i class="btn btn-color delivery-remove" data-id="'+response.id+'">Удалить</i></h2></div></div></div></div>');}
var append_string='',append_block=myJQ('#address-id-'+response.id);append_block.empty();myJQ.each(response.data,function(span_class,span_value){if(span_value.length>0){append_string+='<span class="'+span_class+'">'+span_value+'</span>, ';}});append_block.append(append_string.slice(0,-2));}},error:function(response){myJQ('.loader-gif').hide();console.log(response);}});});myJQ(document).on('click','.delivery-list .delivery-remove',function(e){e.preventDefault();var $this=myJQ(this),url='/udata/data/delivery_del/'+$this.attr('data-id')+'/.json';myJQ.ajax({url:url,dataType:'json',beforeSend:function(){myJQ('.loader-gif').show();},success:function(response){myJQ('.loader-gif').hide();$this.closest('.main-form').remove();},error:function(response){myJQ('.loader-gif').hide();console.log(response);}});});myJQ(document).on('click','.delivery-list .delivery-edit',function(e){e.preventDefault();var delivery_id=myJQ(this).attr('data-id'),$this=myJQ('#address-id-'+delivery_id+' span'),form=myJQ('#delivey-address-form');$this.each(function(){if(myJQ(this).attr('class')!=='order_comments'){form.find('.form-field-wrap.'+myJQ(this).attr('class')+' input').val(myJQ(this).text());}else{form.find('.form-field-wrap.'+myJQ(this).attr('class')+' textarea').val(myJQ(this).text());}});form.find('input[name="address_id"]').val(delivery_id);myJQ('.delivery-form-heading').text('Редактирование адреса доставки');myJQ('.delivery-form-submit').text('Сохранить');myJQ('.delivery-form-cancel').show();myJQ('body,html').animate({scrollTop:form.offset().top-100},1000);});myJQ(document).on('click','.delivery-form-cancel',function(e){e.preventDefault();var form=myJQ('#delivey-address-form');form.find('.form-field-wrap input').val('');form.find('.form-field-wrap textarea').val('');myJQ('#delivey-address-form').find('input[name="address_id"]').val('new');myJQ('.delivery-form-heading').text('Добавление нового адреса доставки');myJQ('.delivery-form-submit').text('Добавить');myJQ('.delivery-form-cancel').hide();});myJQ(document).ready(function(){if(!myJQ("#tabs").find("a").hasClass("tab-Description")){myJQ(".tab-Reviews").click();}});});
/*!
 * jQuery UI Touch Punch 0.2.3
 *
